// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Meter {
    id          Int @default(autoincrement()) @id
    deviceId    String
    deviceType  String
    hubDeviceId String
    humidity    Float
    temperature Float
    createdAt DateTime @default(now())
}
